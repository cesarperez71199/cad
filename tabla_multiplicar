#Funcion que recibe el valor a multiplicar
def numero():
    num=int(input("Dame la tabla a multiplicar: "))
    
    return num


#Funcion que realiza la multiplicacion
def tabla_multiplicar():
    
    for i in range(1,11):
       m=num*i
       
       print(i,"X",num,"=",m)
       
    return m

#Llamado de funciones   
num=numero()

m=tabla_multiplicar()
    

